<p id="notice"><%= notice %></p>

<h1>Listing Base Suppliers</h1>

<table>
  <thead>
    <tr>
      <th>Old supplier</th>
      <th>Code no</th>
      <th>Supplier name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @base_suppliers.each do |base_supplier| %>
      <tr>
        <td><%= base_supplier.old_supplier_id %></td>
        <td><%= base_supplier.code_no %></td>
        <td><%= base_supplier.supplier_name %></td>
        <td><%= link_to 'Show', base_supplier %></td>
        <td><%= link_to 'Edit', edit_base_supplier_path(base_supplier) %></td>
        <td><%= link_to 'Destroy', base_supplier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Base supplier', new_base_supplier_path %>
